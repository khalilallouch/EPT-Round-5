
#include <bits/stdc++.h>
#define N 1000000007;
 
using namespace std;
int main()
{
 ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
 int t;
 cin>>t;
 while(t--)
 {
     int n,m;
     cin>>n>>m;
     long long sum=0;
     int x;
     for(int i=0;i<n;i++)
     {
         cin>>x;
         sum+=x;
     }
     if(sum==m)
        cout<<"Yes";
     else
        cout<<"No";
     cout<<endl;
 }
 return(0);
}
