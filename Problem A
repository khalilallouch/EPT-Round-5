#include <bits/stdc++.h>
#define N 1000000007;
using namespace std;
int main()
{
 ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
 cout.precision(30);
 int t;
 cin>>t;
 while(t--)
 {
     int n;
     cin>>n;
     int k=2;
     while(k<n)
     {
        if(n%(int)(pow(2,k)-1)==0)
            break;
        k++;
     }
     cout<<n/(pow(2,k)-1)<<endl;
 }
 
 return(0);
}
